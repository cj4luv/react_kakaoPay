{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www --exec babel-node localhost",
    "build": "babel ../server -d dist --ignore ./node_modules,./scripts && mkdir -p dist/bin && mkdir -p dist/views && cp ./bin/www ./dist/bin/ && cp -r ./views dist/",
    "serve": "PORT=3002 node dist/bin/www",
    "clean": "rm -rf dist",
    "test": "mocha --compilers js:babel-register"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "iamporter": "^1.4.1",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "request": "^2.81.0",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
